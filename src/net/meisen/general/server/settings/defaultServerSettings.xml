<?xml version="1.0" encoding="UTF-8" ?>

<tdserver xmlns="http://timedata.meisen.net/tdserver/config" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:tdscd="http://timedata.meisen.net/tdserver/config/default"
          xsi:schemaLocation="http://timedata.meisen.net/tdserver/config http://timedata.meisen.net/xsd/tdserver/tdserverConfig.xsd
